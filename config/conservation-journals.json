  "search_strategy": {
    "description": "How to use this journal list for CrossRef searches",
    "approaches": {
      "issn_filter": {
        "method": "Use ISSN to filter CrossRef results",
        "crossref_param": "filter=issn:0888-8892",
        "pros": "High precision, guaranteed journal match",
        "cons": "More API calls required (one per journal)"
      },
      "journal_name_filter": {
        "method": "Use journal name in CrossRef container-title filter",
        "crossref_param": "filter=container-title:Conservation Biology",
        "pros": "Easier to implement, single query",
        "cons": "May miss variations in journal name"
      },
      "hybrid": {
        "method": "Use keyword search + journal verification",
        "description": "Search by keywords, then verify results are from target journals",
        "recommended": true
      }
    }
  },

